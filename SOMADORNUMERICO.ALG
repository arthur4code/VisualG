Algoritmo "somadorNumerico"

var
     n, s, cont:inteiro
inicio
      cont<-1
      enquanto (cont<=4) faca
               escreva("Digite um valor: ")
                  leia(n)
               cont<-cont+1
               s<-s+n
      fimenquanto
      escreval("A soma de todos os valores e: ", s)
fimalgoritmo